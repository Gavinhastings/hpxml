<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="http://hpxml.org/hpxml/2011/1"
	targetNamespace="http://hpxml.org/hpxml/2011/1" elementFormDefault="qualified">
	<xs:include schemaLocation="HPXMLDataTypes.xsd"/>
	<xs:include schemaLocation="BaseElements.xsd"/>
	<xs:element name="ReportTestResultsRequest">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="XMLTransactionHeaderInformation"/>
				<xs:element ref="SoftwareInfo" minOccurs="0"/>
				<xs:element minOccurs="0" name="HomeownerQuestionaire"
					type="HomeownerQuestionaireInfoType">
					<xs:annotation>
						<xs:documentation>Please assign 10 points the reason(s) why you choose to invest in the retrofit. 
You may assign all 10 points to one reason, or distribute the points among several reasons â€“ as long as they total 10.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="BuildingIdentifier" type="RemoteReference"/>
				<xs:element name="TestResults">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="General" minOccurs="0">
								<xs:complexType>
									<xs:sequence>
										<xs:element minOccurs="0" name="OutsideTemperature"
											type="Temperature">
											<xs:annotation>
												<xs:documentation>degrees Farenheit</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element minOccurs="0" name="TestDate" type="TestDate"/>
										<xs:element minOccurs="0" name="TestInorOut"
											type="TestInorOut"/>
										<xs:element minOccurs="0"
											name="NameofIndividualConductingTest" type="xs:string"/>
										<xs:element minOccurs="0"
											name="NameofBusinessConductingTest" type="xs:string"/>
										<xs:element name="Comments" type="xs:string" minOccurs="0"/>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element minOccurs="0" name="HealthAndSafety" type="HealthAndSafety"/>
							<xs:element maxOccurs="unbounded" minOccurs="0"
								name="AirInfiltrationMeasurement"
								type="AirInfiltrationMeasurementType"/>
							<xs:element maxOccurs="unbounded" minOccurs="0"
								name="DuctLeakageMeasurement" type="DuctLeakageMeasurementType"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="ReportTestResultsResponse">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="BuildingIdentifier" type="RemoteReference"/>
				<xs:element maxOccurs="unbounded" minOccurs="0" name="StatusMessages"
					type="StatusMessage"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
</xs:schema>
